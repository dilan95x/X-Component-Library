{
  "name": "@dilan95x/x-component-library",
  "version": "1.0.2",
  "description": "simple library of React components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "format": "prettier --write src/**/*.{ts,tsx}",
    "tsbuild": "tsc",
    "coverage": "npm t -- --coverage --watchAll=false --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.{types,stories,constants,test,spec}.{ts,tsx}'",
    "build": "rollup -c -w",
    "start": "storybook dev -p 6006",
    "test": "jest",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "chromatic": "npx chromatic --project-token=chpt_adc30bd9817d3a6"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "index.tsx",
      "App.tsx",
      "react-app-env.d.ts",
      "reportWebVitals.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dilan95x/X-Component-Library.git"
  },
  "author": {
    "name": "Dillan95x"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dilan95x/X-Component-Library/issues"
  },
  "homepage": "https://github.com/dilan95x/X-Component-Library#readme",
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.5.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/testing-library__jest-dom": "^6.0.0",
    "chromatic": "^11.5.3",
    "globals": "^15.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.1.6",
    "ts-jest": "^29.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-mdx-gfm": "^8.1.6",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-docgen": "^7.0.3",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0"
  }
